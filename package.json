{
  "name": "abrahamcalsin",
  "version": "v3.0.0",
  "description": "my personal website",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "format": "npm run prettier -- --write",
    "check:format": "npm run prettier -- --list-different",
    "clean": "rimraf -rf .next .contentlayer",
    "contentlayer:build": "./node_modules/.bin/contentlayer build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.3.1",
    "@types/react": "17.0.33",
    "clsx": "1.1.1",
    "contentlayer": "^0.1.2",
    "dayjs": "1.10.7",
    "framer-motion": "6.2.8",
    "gray-matter": "4.0.3",
    "next": "^12.0.10",
    "next-contentlayer": "^0.1.2",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-tooltip": "4.2.21",
    "reading-time": "^1.5.0",
    "rehype": "^12.0.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-code-titles": "^1.0.3",
    "rehype-format": "^4.0.1",
    "rehype-prism-plus": "^1.3.2",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "typescript": "4.4.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.14",
    "@types/nprogress": "^0.2.0",
    "@types/remark-prism": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@yonycalsin/prettier-config-react-typescript": "^1.0.1",
    "autoprefixer": "10.2.6",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "postcss": "8.3.5",
    "prettier": "^2.5.1",
    "tailwindcss": "3.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix --max-warnings=0"
  }
}
